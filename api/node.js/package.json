{
  "name": "paddleocrjson",
  "version": "1.1.1",
  "description": "Node.js API based on https://github.com/hiroi-sora/PaddleOCR-json",
  "homepage": "https://github.com/PunchlY/PaddleOCR-json-node-api/blob/main/api/node.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/PunchlY/PaddleOCR-json-node-api.git"
  },
  "author": "punchly <punchly@foxmail.com>",
  "bugs": {
    "url": "https://github.com/PunchlY/PaddleOCR-json-node-api/issues"
  },
  "license": "WTFPL",
  "main": "./esnext",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/__tests__"],
    "testMatch": ["**/*.test.ts", "**/*.test.js"],
    "collectCoverageFrom": [
      "ts/**/*.ts",
      "!ts/**/*.d.ts",
      "!ts/**/*.test.ts"
    ]
  },
  "keywords": [
    "OCR",
    "PaddleOCR",
    "api",
    "json"
  ]
}
